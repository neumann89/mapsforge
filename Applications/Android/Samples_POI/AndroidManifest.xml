<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:installLocation="auto"
	package="org.mapsforge.applications.android.samples" android:versionCode="8" android:versionName="0.3.1-SNAPSHOT">

	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

	<uses-sdk android:minSdkVersion="8" android:targetSdkVersion="17" />

	<application android:allowBackup="true" android:label="Samples_POI">
		<activity android:name=".Samples_POI" android:label="Samples_POI">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".BasicMapViewer" android:label="BasicMapViewer" />
		<activity android:name=".DualMapViewer" android:label="DualMapViewer" />
		<activity android:name=".OverlayMapViewer" android:label="OverlayMapViewer" />
		<activity android:name=".DownloadMapViewer" android:label="DownloadMapViewer" />
		<activity android:name=".OverlayBenchmark" android:label="OverlayBenchmark" />
		<activity android:name=".NearbyPoiMapViewer" android:label="NearbyPoiMapViewer" />
	</application>
</manifest>
